{
  "name": "nextjs-forum",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:bot": "turbo run build --filter=bot...",
    "start": "turbo run start",
    "start:bot": "turbo run start --filter=bot",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "migrate": "yarn workspace @nextjs-discord-forum/db migrate",
    "nuke-db": "turbo run nuke-db",
    "register-commands": "turbo run register-commands",
    "dev:register-commands": "turbo run dev:register-commands",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@nextjs-discord-forum/eslint-config-custom": "*",
    "prettier": "^2.8.4",
    "turbo": "^1.8.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
